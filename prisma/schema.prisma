generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user{
  id String @id @default(uuid())
  userName String @unique
  password String
  myChat Message[]
}

model Message{
  id String @id @default(uuid())
  content String
  responce String
   user user @relation(fields:[userId],references:[id])
   userId String
}



